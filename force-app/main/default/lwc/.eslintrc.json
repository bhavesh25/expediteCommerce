{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",

  "plugins": ["@lwc/eslint-plugin-lwc"],
  "extends": ["@salesforce/eslint-config-lwc/recommended"],
  "overrides": [
    {
      "files": ["*.test.js"],
      "rules": {
        "@lwc/lwc/no-unexpected-wire-adapter-usages": "off"
      }
    }
  ],
  "rules": {
    "no-console": "error"
  }
}
